<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/okmit"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    >

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fdfefe">

            <ImageView
                android:id="@+id/icon3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon2"
                app:layout_constraintTop_toBottomOf="@+id/title" />

            <ImageView
                android:id="@+id/icon4"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon3"
                app:layout_constraintTop_toBottomOf="@+id/title" />

            <ImageView
                android:id="@+id/icon5"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon4"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/icon6"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon5"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/icon1"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/city"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="1.0" />

            <ImageView
                android:id="@+id/icon2"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon1"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="4dp"
                android:maxLength="44"
                android:text="Lauttasaaren liikuntapuisto / Tekojääkaukalo"
                android:textColor="#252a34"
                android:textSize="15.5dp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/city"
                android:layout_width="81dp"
                android:layout_height="21dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="4dp"
                android:maxLength="11"
                android:text="Helsinki"
                android:textColor="#252a34"
                android:textSize="15.5dp"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintEnd_toStartOf="@+id/icon1"
                app:layout_constraintStart_toEndOf="@+id/mapicon"
                app:layout_constraintTop_toBottomOf="@+id/title" />

            <ImageView
                android:id="@+id/mapicon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/ic_baseline_location_on_24"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="1.0"></ImageView>

            <ImageView
                android:id="@+id/icon7"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon6"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="1.0" />

            <ImageView
                android:id="@+id/icon8"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon7"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="1.0" />

            <ImageView
                android:id="@+id/icon9"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="4dp"
                android:contentDescription="TODO"
                app:layout_constraintBottom_toTopOf="@+id/view1"
                app:layout_constraintStart_toEndOf="@+id/icon8"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="0.0" />

            <View
                android:id="@+id/view"
                android:layout_width="6dp"
                android:layout_height="61dp"
                android:background="#306722"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/view1"
                android:layout_width="413dp"
                android:layout_height="2dp"
                android:layout_alignParentRight="true"
                android:background="#252a34"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/icon3" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>